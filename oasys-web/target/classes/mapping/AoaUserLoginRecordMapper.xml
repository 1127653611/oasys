        select r.record_id, r.browser, r.ip_addr, r.login_time, r.session_id, r.user_id,u.user_name from aoa_user_login_record r
        left join aoa_user u on r.user_id=u.user_id
        where
        r.user_id=#{id} order by r.login_time DESC
    </select>
