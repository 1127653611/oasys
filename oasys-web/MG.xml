 <table tableName="aoa_mail_reciver" domainObjectName="AoaMailReciver"></table>
 <table tableName="aoa_notice_list" domainObjectName="AoaNoticeList"></table>
 <table tableName="aoa_in_mail_list" domainObjectName="AoaInMailList"></table>
 <table tableName="aoa_task_user" domainObjectName="AoaTaskUser"></table>
